{
    "name": "bmcclure/drupal-library-mapping",
    "description": "A helper for installing library assets for Drupal.",
    "type": "composer-plugin",
    "license": "GPL-2.0+",
    "require": {
        "php": ">=5.5",
        "composer-plugin-api": "^2.0.0"
    },
    "autoload": {
        "psr-4": {
            "DrupalLibraryMapping\\": "src/"
        }
    },
    "extra": {
        "class": "DrupalLibraryMapping\\Plugin",
        "drupal-library-mapping": {
            "jquery-easing-original": "easing",
            "masonry-layout": "masonry",
            "slick-carousel": "slick"
        }
    },
    "require-dev": {
        "composer/composer": "dev-master"
    }
}
